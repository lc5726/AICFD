{
    inlet
    {
        type            fixedValue;
        value           uniform 298;
    }
    axis
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    asym2
    {
        type            wedge;
    }
    nozzle
    {
         type        externalWallHeatFluxTemperature;
        mode            flux;
         q            5000;//555but/s=.086but/s.in
       // Ta              constant 295.15;
       // h               uniform 10.0;
        thicknessLayers ();
        kappaLayers     ();
        kappaMethod     fluidThermo;
        value           $internalField;
    }
    asym1
    {
        type            wedge;
    }
